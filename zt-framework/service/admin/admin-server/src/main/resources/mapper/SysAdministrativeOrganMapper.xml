    <sql id="selectSysAdministrativeOrganVo">
        select organ_id, name, code, adminName, del_flag, parent_name, phone, create_id, create_time, remark, com_id, ext1, ext2, ext3, ext4, ext5 from sys_administrative_organ
    </sql>
    <!-- 查询对象List -->
    <select id="selectSysAdministrativeOrganList" parameterType="SysAdministrativeOrgan"
            resultMap="sysAdministrativeOrganResult">
        <include refid="selectSysAdministrativeOrganVo"/>
        <where>
            <include refid="equal"/>
        </where>
    </select>

    <!-- 模糊查询对象List -->
    <select id="selectSysAdministrativeOrganListByLike" parameterType="SysAdministrativeOrgan"
            resultType="com.ztman.admin.api.vo.AdministrativeOrganVO">
        select
        ao.organ_id,
        ao.name,
        ao.code,
        ao.adminName,
        ao.phone,
        u.user_realname as create_nmae,
        ao.create_time,
        ao.remark,
        ao.com_id,
        com.name as com_name,
        ao.del_flag,
        ao.ext1,
        ao.ext2,
        ao.ext3,
        ao.ext4,
        ao.ext5
        from
        sys_administrative_organ as ao
        LEFT JOIN
        sys_user u
        ON
        ao.create_id = u.user_id
        LEFT JOIN
        sys_company as com
        ON
        com.com_id = ao.com_id
        where
        ao.del_flag = 1
        <if test="name != null and name != ''">
            AND ao.name like '%${name}%'  <!-- 名称 -->
            OR aa.code like '%${name}%'  <!-- 行政区域 -->
        </if>
        <if test="comId != null and comId != ''">
            and ao.com_id = #{comId}
        </if>
        <if test="code != null and code != ''">
            AND ao.code like '%${code}%'  <!-- 编码 -->
        </if>
        <if test="adminname != null and adminname != ''">
            AND ao.adminName like '%${adminname}%'  <!-- 负责人 -->
        </if>
        <if test="phone != null and phone != ''">
            AND ao.phone like '%${phone}%'  <!-- 负责人联系方式 -->
        </if>
        <if test="remark != null and remark != ''">
            AND ao.remark like '%${remark}%'  <!-- 备注 -->
        </if>
        <if test="ext1 != null and ext1 != ''">
            AND ao.ext1 like '%${ext1}%'  <!--  -->
        </if>
        <if test="ext2 != null and ext2 != ''">
            AND ao.ext2 like '%${ext2}%'  <!--  -->
        </if>
        <if test="ext3 != null and ext3 != ''">
            AND ao.ext3 like '%${ext3}%'  <!--  -->
        </if>
        <if test="ext4 != null and ext4 != ''">
            AND ao.ext4 like '%${ext4}%'  <!--  -->
        </if>
        <if test="ext5 != null and ext5 != ''">
            AND ao.ext5 like '%${ext5}%'  <!--  -->
        </if>
    </select>


