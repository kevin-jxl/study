    <!-- 根据主键查询对象 -->
    <select id="selectSysAdministrativeOrganById" parameterType="Integer" resultType="com.ztman.admin.api.vo.AdministrativeOrganVO">
        select
        ao.organ_id,
        ao.name,
        ao.code,
        ao.adminName,
        ao.phone,
        u.user_realname as create_nmae,
        ao.create_time,
        ao.remark,
        ao.com_id,
        com.name as com_name,
        ao.del_flag,
        ao.ext1,
        ao.ext2,
        ao.ext3,
        ao.ext4,
        ao.ext5
        from
        sys_administrative_organ as ao
        LEFT JOIN
        sys_user u
        ON
        ao.create_id = u.user_id
        LEFT JOIN
        sys_company as com
        ON
        com.com_id = ao.com_id
        where ao.organ_id = #{organId}
    </select>

    <select id="list" parameterType="SysAdministrativeOrgan" resultType="java.util.HashMap">
        select
        organ_id,
        `name`
        from
        sys_administrative_organ
        where
        del_flag = 1
        <if test="companyId != null and companyId != '' or role == 'user'">
           and com_id in (${companyId == null ? 'null' : companyId})
        </if>
    </select>

    <select id="findSysAdministrativeOrganByCode" resultType="SysAdministrativeOrgan">
        select * from sys_administrative_organ where code = #{code} and del_flag = 1
    </select>

    <select id="inputList" resultType="java.util.HashMap">
        select
        organ_id,
        `name`
        from
        sys_administrative_organ
        where
        del_flag = 1
        and com_id = #{comId}
    </select>

    <select id="selectCompanyIdsByUserId" resultType="java.lang.Integer">
        SELECT
        c.com_id
        FROM
        sys_tenant as t
        INNER JOIN
        sys_company as c
        ON
        c.tenant_id = t.tenant_id
        WHERE
        t.admin = #{userId}
        and
        t.del_flag = 1
    </select>


    <update id="updateSysAdministrativeOrgan" parameterType="SysAdministrativeOrgan">
        update sys_administrative_organ
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null  and name != ''  ">name = #{name},</if>
            <if test="code != null  and code != ''  ">code = #{code},</if>
            <if test="adminname != null  and adminname != ''  ">adminName = #{adminname},</if>
            <if test="phone != null  and phone != ''  ">phone = #{phone},</if>
            <if test="createId != null  ">create_id = #{createId},</if>
            <if test="createTime != null  ">create_time = #{createTime},</if>
            <if test="delFlag != null  ">del_flag = #{delFlag},</if>
            <if test="remark != null  and remark != ''  ">remark = #{remark},</if>
            <if test="comId != null  ">com_id = #{comId},</if>
            <if test="ext1 != null  and ext1 != ''  ">ext1 = #{ext1},</if>
            <if test="ext2 != null  and ext2 != ''  ">ext2 = #{ext2},</if>
            <if test="ext3 != null  and ext3 != ''  ">ext3 = #{ext3},</if>
            <if test="ext4 != null  and ext4 != ''  ">ext4 = #{ext4},</if>
            <if test="ext5 != null  and ext5 != ''  ">ext5 = #{ext5},</if>
        </trim>
        where organ_id = #{organId}
    </update>

    <update id="deleteSysAdministrativeOrganById" parameterType="Integer">
        update sys_administrative_organ set del_flag = 0 where organ_id = #{organId}
    </update>

    <delete id="deleteSysAdministrativeOrganByIds" parameterType="String">
        delete from sys_administrative_organ where organ_id in
        <foreach item="organId" collection="array" open="(" separator="," close=")">
            #{organId}
        </foreach>
    </delete>


