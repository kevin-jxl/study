<view><view class="trtc-demo-container"><view class="title" style="{{('padding-top:'+((headerHeight+statusBarHeight)/2-12)+'px')}}"><view>多人会议</view></view><view class="input-box"><input type="number" maxlength="10" placeholder="请输入房间号" placeholder-style="color:#ffffff;opacity: 0.55;" data-event-opts="{{[['input',[['enterRoomID',['$event']]]]]}}" value="{{roomID}}" bindinput="__e"/></view><view class="input-box"><input type="text" maxlength="10" placeholder="请输入用户名(英文+数字)" placeholder-style="color:#ffffff;opacity: 0.55;" data-event-opts="{{[['input',[['enterUserID',['$event']]]]]}}" value="{{userID}}" bindinput="__e"/><button class="btn btn-random" hover-class="btn-hover" data-event-opts="{{[['tap',[['randomUserID',['$event']]]]]}}" bindtap="__e">随机</button></view><view class="choice-content"><text>场景选择</text><radio-group data-event-opts="{{[['change',[['selectScene',['$event']]]]]}}" class="radio-group-choice" bindchange="__e"><label><radio color="#006eff" value="rtc" checked="true" id="open"></radio>通话</label><label><radio color="#006eff" value="live" id="close"></radio>直播</label></radio-group></view><view class="choice-content"><text>本地镜像</text><radio-group data-event-opts="{{[['change',[['selectLocalMirror',['$event']]]]]}}" class="radio-group-no-box" bindchange="__e"><block wx:for="{{localMirrorArray}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label class="{{['radio-item '+(localMirror==item.value?'selected':'')]}}"><radio value="{{item.value}}" checked="{{item.checked}}"></radio>{{item.title}}</label></block></radio-group></view><view class="choice-content"><label class="label" for="switchEarMonitor"><text>开启耳返</text><switch id="switchEarMonitor" color="#006eff" checked="{{enableEarMonitor}}" data-event-opts="{{[['change',[['switchEarMonitor',['$event']]]]]}}" bindchange="__e"></switch></label><label class="label" for="switchAgc"><text>自动增益</text><view class="switch-right"><switch id="switchAgc" color="#006eff" checked="{{enableAgc}}" data-event-opts="{{[['change',[['switchAgc',['$event']]]]]}}" bindchange="__e"></switch></view></label></view><view class="choice-content"><label class="label" for="switchAutoFocus"><text>自动对焦</text><switch id="switchAutoFocus" color="#006eff" checked="{{enableAutoFocus}}" data-event-opts="{{[['change',[['switchAutoFocus',['$event']]]]]}}" bindchange="__e"></switch></label><label class="label" for="switchAns"><text>噪声消除</text><view class="switch-right"><switch id="switchAns" color="#006eff" checked="{{enableAns}}" data-event-opts="{{[['change',[['switchAns',['$event']]]]]}}" bindchange="__e"></switch></view></label></view><view class="choice-content"><label class="label" for="switchLocalVideo"><text>本地视频</text><switch id="switchLocalVideo" color="#006eff" checked="{{localVideo}}" data-event-opts="{{[['change',[['switchLocalVideo',['$event']]]]]}}" bindchange="__e"></switch></label><label class="label" for="switchLocalAudio"><text>本地音频</text><view class="switch-right"><switch id="switchLocalAudio" color="#006eff" checked="{{localAudio}}" data-event-opts="{{[['change',[['switchLocalAudio',['$event']]]]]}}" bindchange="__e"></switch></view></label></view><view class="choice-content"><label class="label" for="switchDebugMode"><text>调试模式</text><switch id="switchDebugMode" color="#006eff" checked="{{debugMode}}" data-event-opts="{{[['change',[['switchDebugMode',['$event']]]]]}}" bindchange="__e"></switch></label><label class="label" for="switchSmallScreen"><text>启用小画面</text><view class="switch-right"><switch id="switchSmallScreen" color="#006eff" checked="{{encsmall}}" data-event-opts="{{[['change',[['switchSmallScreen',['$event']]]]]}}" bindchange="__e"></switch></view></label></view><view class="choice-content"><text class="device-position">摄像头选择</text><radio-group data-event-opts="{{[['change',[['selectDevicePosition',['$event']]]]]}}" class="radio-group-choice" bindchange="__e"><label><radio color="#006eff" value="front" checked="true" id="open"></radio>前置</label><label><radio color="#006eff" value="back" id="close"></radio>后置</label></radio-group></view><view class="choice-content"><text>视频分辨率</text><radio-group data-event-opts="{{[['change',[['selectResolution',['$event']]]]]}}" class="radio-group-no-box" bindchange="__e"><block wx:for="{{resolutionArray}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label class="{{['radio-item '+(resolution==item.value?'selected':'')]}}"><radio value="{{item.value}}" checked="{{item.checked}}"></radio>{{item.title}}</label></block></radio-group></view></view><view class="bottom-btn"><button type="primary" hover-class="none" data-event-opts="{{[['tap',[['enterRoom',['$event']]]]]}}" bindtap="__e">进入房间</button></view><cover-image class="close" style="{{('top:'+(headerHeight+statusBarHeight-34)+'rpx')}}" src="../../images/back.png" data-event-opts="{{[['tap',[['onBack',['$event']]]]]}}" bindtap="__e"></cover-image></view>